style.
  /*
   *  COLORS
   * #121315 -- dark
   * #1A1A1C -- lighter
   * #607D8B -- blue
   * #2a2b2d -- hr
   */

  html, body {
    background-color: #121315;
  }

  #mainNavbar {
    background-color: #1A1A1C;
    border-color: #1A1A1C;
  }

  #mainNavbar a {
    color: #FFF;
  }

  body {
    padding-top: 60px;
  }

  label {
    color: #FFF;
  }

  .event-col {
    color: #FFF;
    border-bottom: solid 1px #2a2b2d;
    padding-bottom: 8px;
    padding-top: 8px;
  }

  .event-col:hover {
    background-color: #1A1A1C;
  }

  /*.event-row {
    margin-bottom: 5px;
  }*/

  .round-img img {
    border-radius: 50%;
  }

  #event-row-1, #event-row-15, #event-row-2, #event-row-3 {
    color: #FFF;
  }

  #event-row-15 {
    padding-top: 10px;
    padding-bottom: 20px;
    font-size: 24px;
    font-weight: bolder;
    text-align: center;
  }

  #event-row-1 {
    padding-bottom: 10px;
    border-bottom: solid 1px #2a2b2d;
  }

  #event-row-2 {
    padding-top: 10px;
    padding-bottom: 10px;
    border-bottom: solid 1px #2a2b2d;
  }

  #event-row-3 {
    padding-top: 10px;
  }

  #hostInfo {
    font-size: 24px;
  }

  #placeInfo {
    font-weight: bold;
    padding-top: 10px;
  }

  #dateInfo {
    padding-top: 5px;
    color: #DEDEDE;
  }

  #subscribeButton, #unsubscribeButton {
    background-color: #1A1A1C;
    border-color: #FFF;
    color: #FFF;
  }

  .member-row {
    padding-bottom: 10px;
  }